# AWS credentials
AWS_ACCESS_KEY_ID=               # Your AWS access key ID
AWS_SECRET_ACCESS_KEY=           # Your AWS secret access key

# AWS region where your resources (e.g., S3 bucket) are located
AWS_REGION=                      # e.g., us-east-1

# S3 bucket configuration
S3_BUCKET_NAME=                  # Name of your S3 bucket

# Input folder inside the S3 bucket (e.g., where source files are uploaded)
S3_INPUT_FOLDER=input            # Recommended: create an 'input' folder in your bucket

# Output folder inside the S3 bucket (e.g., where processed files will be saved)
S3_OUTPUT_FOLDER=output          # Recommended: create an 'output' folder in your bucket


